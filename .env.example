# Log level (info, debug, trace)
RUST_LOG=info

# URL of the Nostr relay
NOSTR_PUSH__NOSTR__RELAY_URL=ws://host.docker.internal:8080

# Private key for the service identity (optional)
# NOSTR_PUSH__SERVICE__PRIVATE_KEY_HEX=

# Redis connection URL
NOSTR_PUSH__REDIS__URL=redis://redis:6379

# Google Cloud Project ID for FCM
NOSTR_PUSH__FCM__PROJECT_ID=pub-verse-app